<% provide(:title, 'All users') %>
<h1>Current users of the site:</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <!--<%= debug session %>-->
      <%= gravatar_for user, size: 25 %>
      <%= link_to user.username, @user %>
      <%= link_to user.email, @user %>

    </li>
  <% end %>
</ul>
